{
    "name": "survos/fw-bundle",
    "description": "Bundle with utilities for creating a Symfony-based mobile app with framework7",
    "keywords": [
        "symfony-ux"
    ],
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "php": "^8.4",
        "symfony/config": "^7.3||^8.0",
        "symfony/dependency-injection": "^7.3||^8.0",
        "symfony/http-kernel": "^7.3||^8.0",
        "twig/twig": "^3.4",
        "survos/core-bundle": "^2.0",
        "knplabs/knp-menu-bundle": "^3.2",
        "symfony/event-dispatcher": "^7.3",
        "symfony/ux-twig-component": "^2.8",
        "symfony/options-resolver": "^7.3||^8.0",
        "symfony/console": "^7.3||^8.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "symfony/browser-kit": "^7.3||^8.0",
        "symfony/framework-bundle": "^7.3||^8.0",
        "symfony/phpunit-bridge": "^7.3||^8.0",
        "symfony/twig-bundle": "^7.3||^8.0",
        "symfony/var-dumper": "^7.3||^8.0",
        "symfony/security-bundle": "^7.3||^8.0",
        "survos/auth-bundle": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Survos\\FwBundle\\": "src/"
        }
    },
    "suggest": {
        "intl-extra": "For better performance, install the intl-extra package to use the IntlDateFormatter class."
    },
    "config": {
        "allow-plugins": {
            "survos/installer": true
        }
    }
}
